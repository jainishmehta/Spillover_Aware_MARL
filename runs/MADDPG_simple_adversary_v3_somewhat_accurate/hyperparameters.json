{
    "env_name": "simple_adversary_v3",
    "algo": "MADDPG",
    "total_timesteps": 400000,
    "buffer_size": 1000000,
    "warmup_steps": 20000,
    "batch_size": 512,
    "max_steps": 25,
    "gamma": 0.95,
    "tau": 0.01,
    "actor_lr": 0.001,
    "critic_lr": 0.002,
    "hidden_sizes": "64,64",
    "update_every": 15,
    "noise_scale": 0.2,
    "min_noise": 0.05,
    "eval_interval": 10000,
    "collect_trajectory": true,
    "trajectory_interval": 1000,
    "reward_scale": 1.0,
    "weight_decay": 0.0001,
    "grad_clip_norm": 0.5,
    "adversary_actor_lr": 0.0005,
    "adversary_critic_lr": 0.001,
    "separate_buffers": false,
    "curriculum_learning": false,
    "reward_shaping": false,
    "maintain_coupling": true,
    "lr_decay_threshold": 10000,
    "lr_decay_factor": 0.5,
    "persistent_exploration": true
}